<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/custom_button"
    android:layout_margin="20dp"
    android:orientation="vertical"
    android:layout_gravity="center_vertical|center_horizontal"
     >

    <LinearLayout
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >
        
        <ImageView 
            android:id="@+id/oneLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        <ImageView 
            android:id="@+id/twoLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        <ImageView 
            android:id="@+id/threeLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tableRow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >
        
         <ImageView 
            android:id="@+id/fourLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        <ImageView 
            android:id="@+id/fiveLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />

                <ImageView 
            android:id="@+id/sixLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        
    </LinearLayout>

    
   <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >
        
                <ImageView 
                    android:id="@+id/sevenLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        <ImageView 
            android:id="@+id/eightLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        <ImageView 
            android:id="@+id/nineLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        
    </LinearLayout>
    
   
       
   <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >
        
                <ImageView 
                    android:id="@+id/tenLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        <ImageView 
            android:id="@+id/elevenLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        <ImageView 
            android:id="@+id/emptyLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            />
        
        
    </LinearLayout>
    
   
   
        <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
         >
        
                <ImageView 
                    android:id="@+id/startLev2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:layout_gravity="center_horizontal"
            
            />
        
        
        
    </LinearLayout>
    
        

</LinearLayout>
